<div id="content" class="container_16">
    	<!-- begin main content area -->
        <div id="page_content" class="grid_11 alpha">
        	<h3 class="content_title">Remove <%= @notice.nhead %> from the Table</h3>
        	
<% if !flash[:notice].blank? %>
<div>
	<p align = "center"><%= flash[:notice] %></p>
</div>
<% end %>   

<div>
	<%= form_for(:notice, :url => {:action => 'destroy', :id => @notice.id}) do |f| -%>
</div>
<p align = "center">Are you sure you want to permanently remove <%= @notice.nhead %> from the table?</p>
	<div class = "actions" >
  		<%= submit_tag("Delete") %>
  	</div>
<% end %>
 <div>
    	<p align="right"><%= link_to("Click to go Back", {:action => 'index'}) %><br/><br/></p>
    </div>
</div>
</div>